<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>login</web-resource-name>
      <url-pattern>/*</url-pattern>
    </web-resource-collection>
    <user-data-constraint>
      <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
  </security-constraint>
  <security-constraint>
    <display-name>JSP Protection</display-name>
    <web-resource-collection>
      <web-resource-name>SecureJSPPages</web-resource-name>
      <url-pattern>*.jsp</url-pattern>
    </web-resource-collection>
    <auth-constraint/>
  </security-constraint>
  <servlet>
    <servlet-name>loginInternalName</servlet-name>
    <servlet-class>servletAccount.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginInternalName</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>joinInternalName</servlet-name>
    <servlet-class>servletAccount.Join</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>joinInternalName</servlet-name>
    <url-pattern>/join.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>articleWriteInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleWrite</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleWriteInternalName</servlet-name>
    <url-pattern>/articleWrite.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>articleListInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleListInternalName</servlet-name>
    <url-pattern>/articleList.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>articleViewInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleView</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleViewInternalName</servlet-name>
    <url-pattern>/articleView.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>articleModifyInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleModify</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleModifyInternalName</servlet-name>
    <url-pattern>/articleModify.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>articleModifyCompleteInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleModifyComplete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleModifyCompleteInternalName</servlet-name>
    <url-pattern>/articleModifyComplete.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>articleDeleteInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleDelete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleDeleteInternalName</servlet-name>
    <url-pattern>/articleDelete.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>firstPageInternalName</servlet-name>
    <servlet-class>servletAccount.FirstPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>firstPageInternalName</servlet-name>
    <url-pattern>/first</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logoutInternalName</servlet-name>
    <servlet-class>servletAccount.LogOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logoutInternalName</servlet-name>
    <url-pattern>/logout.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>articleWriteFormInternalName</servlet-name>
    <servlet-class>servletBoard.ArticleWriteForm</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>articleWriteFormInternalName</servlet-name>
    <url-pattern>/articleWriteForm.do</url-pattern>
  </servlet-mapping>
  
</web-app>